
public abstract class  Node {
	static int NumberType = 0;
	static int UnknownType = 1;
	
	static int OPERATION_ADD = 1;
	static int OPERATION_SUB = 2;
	static int OPERATION_MUL = 3;
	static int OPERATION_DIV = 4;
	static int OPERATION_INC = 5;
	static int OPERATION_DEC = 6;
	
	protected int type;
	protected String value;
	
	public Node(int type) {
		this.type = type;
	}
	
	public int getType() {
		return type;
	}
	public abstract void setValue(int value); 
	public abstract int getValue(); 
	
	public abstract void setOperation(); 
	
	
}
